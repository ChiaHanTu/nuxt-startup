{
  "name": "nuxt-app",
  "packageManager": "pnpm@8.4.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "typecheck": "nuxi typecheck",
    "lint:script": "eslint --ext .ts,.js,.vue --max-warnings=0",
    "lint:style": "stylelint '**/*.{vue,scss}' --ignore-path .gitignore node_modules",
    "lint:script:fix": "pnpm lint:script --fix",
    "lint:style:fix": "pnpm lint:style --fix",
    "lint": "pnpm lint:script && pnpm lint:style"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.18.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@unocss/eslint-config": "^0.53.6",
    "@unocss/nuxt": "^0.53.6",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.2.2",
    "unocss": "^0.53.6",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.22"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "@unocss/reset": "^0.53.6",
    "@vorms/core": "^1.1.0",
    "@vorms/resolvers": "^1.1.0",
    "nuxt": "3.7.4",
    "pinia": "^2.1.7",
    "vue-final-modal": "^4.4.5",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "./**/*.{js,ts,vue}": [
      "eslint --max-warnings=0"
    ],
    "./**/*.{scss,vue}": [
      "stylelint"
    ]
  }
}
